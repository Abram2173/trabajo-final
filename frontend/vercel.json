{
    "version": 2,
    "builds": [
      { "src": "backend/server.js", "use": "@vercel/node" },
      { "src": "frontend/build/**", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "backend/server.js" },
      { "src": "/(.*)", "dest": "frontend/build/index.html" }
    ],
    "env": {
      "MYSQL_HOST": "localhost",
      "MYSQL_USER": "root",
      "MYSQL_PASSWORD": "",
      "MYSQL_DATABASE": "signup"
    }
  }
  